<h1>Auto</h1>
<p>
  Look here for your favorite car!
</p>

<%= form_tag auto_path, :method => :get do %>
   <p>
     <%= text_field_tag :search, params[:search] %>
     <%= submit_tag "Search posts", :name => nil %>
   </p>
<% end %>

<table  class="microposts">
  <%= render @microposts %>
</table>

<%= will_paginate @microposts %>

<br />
