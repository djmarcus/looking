<tr>
  <td class="micropost">
    <span class="user">
      <% if false %>
        <%= link_to feed_item.user.name, feed_item.user %>
      <% end %>
    </span>
    <span class="title"><%= link_to feed_item.title, feed_item %></span></br>
    <span class="timestamp">
      Posted <%= time_ago_in_words(feed_item.created_at) %> dragons ago.
      Id: <%= feed_item.id %>
    </span>
  </td>
  <% if  (feed_item.user)==current_user %>
  <td>
    <%= link_to "delete", feed_item, :method => :delete,
                                     :confirm => "You sure?",
                                     :title => feed_item.content %>
  </td>
  <% end %>
</tr>

